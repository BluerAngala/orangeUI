/* pages/comp/initialQuery/initialQuery.wxss */
.nav{
  position: fixed;
  top: 6%;
  right: 15rpx;
  width: 30rpx;
  font-size: 26rpx;
  color: #333;
  text-align: center;
  background: rgba(0, 0, 0, 0.2);
  border-radius: 30rpx;
  padding: 3px 0;
}
.active{
  color: #ff5b1f;
}

.main{
  height: 100%;
  overflow-y: scroll;
}
.initial-item .title{
  height: 30px;
  line-height: 30px;
  padding-left: 20px;
  font-size: 12px;
  background: #f4f5f5;
}
.singer-item{
  padding: 20rpx 0 0 30rpx;
  display: flex;
  align-items: center;
}
.singer-item image{
  width: 50px;
  height: 50px;
  border-radius: 50%;
  display: block;
}
.singer-item text{
  margin-left: 20rpx;
}